generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @default(autoincrement()) @id
  email     String  @unique
  login     String
  password  String
}

model Product {
  id            Int     @default(autoincrement()) @id
  name          String  @unique
  description   String
  image         String
  imageSmall    String
  catalog       String
  price         Float
}
